{
    "name": "groton-school/slim-gae-shim",
    "description": "Shim for Slim Skelton to run on Google App Engine",
    "authors": [
        {
            "name": "Seth Battis",
            "email": "sbattis@groton.org"
        }
    ],
    "license": "GPL-3.0",
    "require": {
        "composer/composer": "^2.8",
        "google/cloud-logging": "^1.32",
        "groton-school/slim-norms": "^2.1",
        "php-di/php-di": "^7.0",
        "psr/container": "^2.0",
        "psr/http-message": "^2.0",
        "psr/log": "^3.0",
        "slim/slim": "^4.12",
        "symfony/filesystem": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "GrotonSchool\\Slim\\GAE\\": "src/"
        }
    },
    "scripts": {
        "release": "conventional-changelog --commit"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.76",
        "marcocesarato/php-conventional-changelog": "^1.17"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "2.1.0"
}